{
  "name": "@marshal/mongo",
  "version": "0.1.2",
  "description": "Marshal's Mongo database management.",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "tsc": "tsc"
  },
  "repository": "https://github.com/marcj/marshaller",
  "author": "Marc J. Schmidt <marc@marcjschmidt.de>",
  "license": "MIT",
  "dependencies": {
    "mongodb": "^3.1.8",
    "@marshal/core": "^0.1.0"
  },
  "devDependencies": {
    "@types/mongodb": "^3.1.12"
  },
  "jest": {
    "testURL": "http://localhost/",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "**/*.{ts}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/tests/**.test.ts"
    ],
    "setupTestFrameworkScriptFile": "jest-extended"
  }
}
